{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node: Nodemon",
      "runtimeExecutable": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
      "program": "${workspaceFolder}/dist/main.js",
      "restart": true,
      "sourceMaps": true,
      "preLaunchTask": "webpack",
      "protocol": "inspector"
    },
    {
      "name": "Docker Node.js Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "platform": "node"
    }
  ]
}